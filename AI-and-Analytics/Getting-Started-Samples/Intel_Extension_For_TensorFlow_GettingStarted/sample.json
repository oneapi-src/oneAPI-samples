	"name": "Intel® Extension for TensorFlow* Getting Started",
	"categories": ["Toolkit/oneAPI AI And Analytics/Features And Functionality"],
	"description": "This code sample will guide users how to run a TensorFlow inference workload on both the GPU and CPU by using the Intel® AI Analytics Toolkit.",
	"builder": ["cli"],
	"languages": [{
		"python": {}
	}],
	"os": ["linux"],
	"targetDevice": ["CPU", "GPU"],
	"gpuRequired": ["ats-m", "pvc"],
	"ciTests": {
		"linux": [{
			"env": [
				"pixi shell --manifest-path ../../Preset_environments/deep_learning_tensorflow_cpu/pixi.toml",
				"pixi add --pypi --feature test_sample pip",
				"pixi project environment add testcpu --feature test_sample",
				"pixi shell -e testcpu",
				"pip install -r requirements.txt --no-deps",
				"pip install jupyter ipykernel",
				"python -m ipykernel install --user --name=tensorflow",

				"pixi shell --manifest-path ../../Preset_environments/deep_learning_tensorflow_gpu/pixi.toml",
				"pixi add --pypi --feature test_sample pip",
				"pixi project environment add testgpu --feature test_sample",
				"pixi shell -e testgpu",
				"pip install -r requirements.txt --no-deps",
				"pip install jupyter ipykernel",
				"python -m ipykernel install --user --name=tensorflow-gpu"
			],
			"id": "itex_sample_test",
			"steps": [
				"pixi shell --manifest-path ../../Preset_environments/deep_learning_tensorflow_cpu/pixi.toml",
				"pixi shell -e testcpu",
				"jupyter nbconvert --ExecutePreprocessor.enabled=True --ExecutePreprocessor.kernel_name=tensorflow --to notebook --execute ResNet50_Inference.ipynb --output ResNet50_Inference.nbcovert.cpu.ipynb",
				"pixi shell -e default",
				"pixi project environment remove testcpu",
				"pixi remove --pypi --feature test_sample 'pip'",
				"rm -rf ../../Preset_environments/deep_learning_tensorflow_cpu/.pixi/envs/test",
				
				"pixi shell --manifest-path ../../Preset_environments/deep_learning_tensorflow_gpu/pixi.toml",
				"pixi shell -e testgpu",
				"jupyter nbconvert --ExecutePreprocessor.enabled=True --ExecutePreprocessor.kernel_name=tensorflow-gpu --to notebook --execute ResNet50_Inference.ipynb --output ResNet50_Inference.nbcovert.gpu.ipynb",
				"pixi project environment remove testgpu",
				"pixi remove --pypi --feature test_sample 'pip'",
				"rm -rf ../../Preset_environments/deep_learning_tensorflow_gpu/.pixi/envs/test"
			]
		}]
	},
	"expertise": "Code Optimization"
