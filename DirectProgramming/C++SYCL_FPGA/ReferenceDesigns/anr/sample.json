{
  "guid": "B2974A99-4BC6-4F3E-AD3E-C17FE86AC2A4",
  "name": "Adaptive Noise Reduction",
  "categories": ["Toolkit/oneAPI Direct Programming/C++SYCL FPGA/Reference Designs"],
  "description": "A highly optimized adaptive noise reduction (ANR) algorithm on an FPGA.",
  "toolchain": ["icpx"],
  "os": ["linux", "windows"],
  "builder": ["ide", "cmake"],
  "targetDevice": ["FPGA"],
  "languages": [{"cpp":{}}],
  "commonFolder": {
    "base": "../..",
    "include": [
      "README.md",
      "ReferenceDesigns/anr",
      "include"
    ],
    "exclude": []
  },
  "ciTests": {
    "linux": [
      {
        "id": "fpga_emu",
        "steps": [
          "icpx --version",
          "mkdir build",
          "cd build",
          "cmake ..",
          "make fpga_emu",
          "./anr.fpga_emu"
        ]
      },
      {
        "id": "report",
        "steps": [
          "icpx --version",
          "mkdir build",
          "cd build",
          "cmake ..",
          "make report"
        ]
      }
    ],
    "windows": [
      {
        "id": "fpga_emu",
        "steps": [
          "icpx --version",
          "cd ../..",
          "mkdir build",
          "cd build",
          "xcopy /E ..\\ReferenceDesigns\\anr\\test_data ..\\test_data\\",
          "cmake -G \"NMake Makefiles\" ../ReferenceDesigns/anr",
          "nmake fpga_emu",
          "anr.fpga_emu.exe"
        ]
      },
      {
        "id": "report",
        "steps": [
          "icpx --version",
          "cd ../..",
          "mkdir build",
          "cd build",
          "xcopy /E ..\\ReferenceDesigns\\anr\\test_data ..\\test_data\\",
          "cmake -G \"NMake Makefiles\" ../ReferenceDesigns/anr",
          "nmake report"
        ]
      }
    ]
  },
  "expertise": "Reference Designs and End to End"
}
