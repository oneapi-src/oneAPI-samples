  "ciTests": {
    "linux": [
      {
        "id": "fpga_emu",
        "steps": [
          "icpx --version",
          "mkdir build",
          "cd build",
          "cmake ..",
          "make fpga_emu",
          "./fpga_template.fpga_emu"
        ]
      },
      {
        "id": "report",
        "steps": [
          "icpx --version",
          "mkdir build",
          "cd build",
          "cmake ..",
          "make report"
        ]
      }
    ],
    "windows": [
      {
        "id": "fpga_emu",
        "steps": [
          "icpx --version",
          "cd ../../..",
          "mkdir build",
          "cd build",
          "cmake -G \"NMake Makefiles\" ../Tutorials/GettingStarted/fpga_template",
          "nmake fpga_emu",
          "fpga_template.fpga_emu.exe"
        ]
      },
      {
        "id": "report",
        "steps": [
          "icpx --version",
          "cd ../../..",
          "mkdir build",
          "cd build",
          "cmake -G \"NMake Makefiles\" ../Tutorials/GettingStarted/fpga_template",
          "nmake report"
        ]
      }
    ]
